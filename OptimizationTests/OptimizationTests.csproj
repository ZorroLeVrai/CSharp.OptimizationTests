
<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Exercices\CSV_Files\football_players.csv" />
    <None Remove="Exercices\CSV_Files\tennis_players.csv" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Exercices\CSV_Files\football_players.csv">
